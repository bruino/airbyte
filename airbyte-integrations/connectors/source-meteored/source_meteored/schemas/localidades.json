                                    },
                                    "temp": {
                                        "type": "string"
                                    },
                                    "symbol_value": {
                                        "type": "string"
                                    },
                                    "symbol_description": {
                                        "type": "string"
                                    },
                                    "symbol_value2": {
                                        "type": "string"
                                    },
                                    "symbol_description2": {
                                        "type": "string"
                                    },
                                    "wind": {
                                        "type": "object",
                                        "properties": {
                                            "speed": {
                                                "type": "string"
                                            },
                                            "dir": {
                                                "type": "string"
                                            },
                                            "symbol": {
                                                "type": "string"
                                            },
                                            "symbolB": {
                                                "type": "string"
                                            },
                                            "gusts": {
