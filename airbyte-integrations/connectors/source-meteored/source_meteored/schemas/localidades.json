                                        "type": "string"
                                    },
                                    "windchill": {
                                        "type": "string"
                                    },
                                    "uv_index": {
                                        "type": "string"
                                    }
                                },
                                "required": [
                                    "interval",
                                    "temp",
                                    "symbol_value",
                                    "symbol_description",
                                    "symbol_value2",
                                    "symbol_description2",
                                    "wind",
                                    "rain",
                                    "humidity",
                                    "pressure",
                                    "clouds",
                                    "snowline",
                                    "windchill",
                                    "uv_index"
                                ]
                            }
                        }
                    },
                    "required": [
                        "date",
                        "name",
                        "symbol_value",
                        "symbol_description",
                        "symbol_value2",
                        "symbol_description2",
                        "tempmin",
                        "tempmax",
                        "wind",
                        "rain",
                        "humidity",
                        "pressure",
                        "snowline",
                        "uv_index_max",
                        "sun",
                        "moon",
                        "units",
                        "local_time",
                        "local_time_offset",
