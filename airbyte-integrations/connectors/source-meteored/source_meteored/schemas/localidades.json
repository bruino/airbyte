                        "units": {
                            "type": "object",
                            "properties": {
                                "temp": {
                                    "type": "string"
                                },
                                "wind": {
                                    "type": "string"
                                },
                                "rain": {
                                    "type": "string"
                                },
                                "pressure": {
                                    "type": "string"
                                },
                                "snowline": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "temp",
                                "wind",
                                "rain",
                                "pressure",
                                "snowline"
                            ]
